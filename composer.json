{
  "name": "ervinomueller/l5-repository",
  "description": "Laravel 5 - Repositories to the database layer (Fork of andersao/l5-repository)",
  "keywords": ["laravel", "repository", "eloquent", "model", "cache"],
  "license": "MIT",
  "authors": [
    {
      "name": "Ervino Mueller",
      "homepage": "https://ervinomueller.com",
      "email": "ervinomueller@outlook.com",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/ervinomueller/l5-repository",
  "support": {
    "email": "ervinomueller@outlook.com",
    "issues":"https://github.com/ervinomueller/l5-repository/issues",
    "wiki":"https://github.com/ervinomueller/l5-repository",
    "source":"https://github.com/ervinomueller/l5-repository",
    "docs": "https://github.com/ervinomueller/l5-repository"
  },
  "require": {
    "illuminate/http": "~7.0",
    "illuminate/config": "~7.0",
    "illuminate/support": "~7.0",
    "illuminate/database": "~7.0",
    "illuminate/pagination": "~7.0",
    "illuminate/console": "~7.0",
    "illuminate/filesystem": "~7.0",
    "prettus/laravel-validation": "~1.2",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Prettus\\Repository\\": "src/Prettus/Repository/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "suggest": {
    "league/fractal": "Required to use the Fractal Presenter (0.12.*).",
    "robclancy/presenter": "Required to use the Presenter Model (1.3.*)",
    "prettus/laravel-validation": "Required to provide easy validation with the repository (1.1.*)"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Prettus\\Repository\\Providers\\RepositoryServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
